{
    "compileOnSave": false,
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "preserve",
      "allowJs": false,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "noImplicitAny": true,
      "noImplicitReturns": false,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "strict": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "typeRoots": [
        "./node_modules/@types"
      ],
      "lib": [
        "dom",
        "es2015",
        "es2016"
      ]
    }
}
